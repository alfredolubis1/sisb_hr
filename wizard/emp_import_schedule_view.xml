<?xml version='1.0' encoding='UTF-8'?>
<openerp>
    <data>
    
        <record id="sisb_emp_import_schedule_wizard_view" model="ir.ui.view">
            <field name="name">Import Schedule</field>
            <field name="model">sisb.emp.import.schedule</field>
            <field name="arch" type="xml">
                <form string="Import Employee's">
                    <group col="4" colspan="4">
                        <field name="name"/>
                        <newline/>
                        <!-- <field name="msg" colspan="4" nolabel="1" width="650" height="400"/> -->
                    </group>
                    <footer>
                        <button name="import_employee_schedule" string="Import" type="object" icon="gtk-yes" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>            
		        </form>
            </field>
        </record>

        <record id="action_import_employee" model="ir.actions.act_window">
            <field name="name">Import Schedule</field>
            <field name="res_model">sisb.emp.import.schedule</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="sisb_emp_import_schedule_wizard_view"/>
            <field name="target">new</field>
        </record>
            
    	<!--<act_window name="Import Lines" res_model="wizard.import" src_model="sale.order" view_mode="form" target="new" key2="client_action_multi" id="action_sale_line_import" groups="group_wizard_import_line"/>-->
        </data>
</openerp>
